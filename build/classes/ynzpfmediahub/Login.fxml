<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ynzpfmediahub.LoginController">
   <children>
      <Text layoutX="88.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="MediaHub">
         <font>
            <Font name="Apple Color Emoji" size="38.0" />
         </font>
      </Text>
      <GridPane layoutX="161.0" layoutY="155.0" prefHeight="116.0" prefWidth="308.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="91.0" />
